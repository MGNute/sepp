[data]
molecule=dna

[commandline]
tree = 16S_bacteria.refpkg/sate.taxonomy
alignment = rpsB.refpkg/sate.fasta
raxml = rpsB.refpkg/sate.taxonomy.RAxML_info
alignmentSize = 100
fragment = fragments.fas
output = rpsB_run

[exhaustive]
strategy = centroid
minsubsetsize = 2
sepp_alignment_threshold = 0
weight_placement_by_alignment = false
placer = pplacer

[tipp]
push_down = true
alignmentThreshold = 0.95 
placementThreshold = 0.95 
taxonomy = rpsB.refpkg/all_taxon.taxonomy 
taxonomyNameMapping= rpsB.refpkg/species.mapping 
alignmentDecompositionTree = rpsB.refpkg/sate.tree.ml 

